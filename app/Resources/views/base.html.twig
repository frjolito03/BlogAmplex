            </div>
            <!-- script for menu -->
            <script>
             //   $( "span.menu" ).click(function() {
               //     $( ".list-nav" ).slideToggle( "slow", function() {
                        // Animation complete.
                 //   });
               // });
            </script>
            <!-- script for menu -->

            <div class="clearfix"></div>
        </div>
    </div>
    {% block container %}
    <div class="container">
        <div class="content">
            <div class="col-md-7 content-left">
                <h5 class="head">En noticias Recientes</h5>
                {% for allpost in allpost %}
                <div class="article">
                    <h6>Software</h6>
                    <a class="title" href="{{ path('printig', {'id': allpost.id}) }}"">{{ allpost.titulo }}</a>
                    <a href="{{ path('printig', {'id': allpost.id}) }}"><img src="../uploads/{{ allpost.imagen }}" alt="" /></a>
                    <p>{{ allpost.contenido|slice(0, 50) }}. </p>

                </div>
                {% endfor %}

            </div>
            <div class="col-md-5 content-right">
                <div class="content-right-top">
                    <h5 class="head">Popular</h5>
                    {% for popular in popular %}
                    <a href="{{ path('printig', {'id': popular.id}) }}">
                        <div class="editor text-center">
                            <h3>{{ popular.titulo  }}</h3>
                            <p>{{ popular.contenido|slice(0, 20) }}.</p>
                            <label></label>
                            <span></span>
                        </div>
                    </a>
                    {% endfor %}

                </div>
                <div class="editors-pic-grids">
                    <h5>Seleccion de editores</h5>
                    {% for editores in editores %}
                    <div class="editors-pic">
                        <div class="e-pic">
                            <a href="{{ path('printig', {'id': editores.id}) }}"><img src="../uploads/{{ editores.imagen }}" alt="" /></a>
                        </div>
                        <div class="e-pic-info">
                            <a href="{{ path('printig', {'id': editores.id}) }}">{{ editores.contenido|slice(0, 10) }}</a>
                            <span></span>
                            <label></label>
                        </div>
                        <div class="clearfix"></div>
                    </div>
                    {% endfor %}
                </div>
            </div>
            <div class="clearfix"></div>
            <div class="features" style="background-image:url('../uploads/{{ feacture.imagen }}')">
                <h5>Lo Mas Nuevo</h5>
                <h2>{{ feacture.titulo|slice(0,25 ) }}</h2>
            </div>
            <div class="col-md-7 content-left">
                <div class="article">
                    <h5 class="head">En noticias recientes</h5>
                    {% for reciente2 in reciente2 %}
                        <div class="article">
                            <h6>Software</h6>
                            <a class="title" href="{{ path('printig', {'id': reciente2.id}) }}"">{{ reciente2.titulo }}</a>
                            <a href="{{ path('printig', {'id': reciente2.id}) }}"><img src="../uploads/{{ reciente2.imagen }}" alt="" /></a>
                            <p>{{ reciente2.contenido|slice(0, 50) }}. </p>

                        </div>
                    {% endfor %}                </div>
            </div>
            <div class="col-md-5 content-right content-right-top">
                <h5 class="head">Popular</h5>
                    {% for popular2 in popular2 %}
                        <a href="{{ path('printig', {'id': popular2.id}) }}">
                            <div class="editor text-center">
                                <h3>{{ popular2.titulo  }}</h3>
                                <p>{{ popular2.contenido|slice(0, 15) }}.</p>
                                <label></label>
                                <span></span>
                            </div>
                        </a>
                    {% endfor %}

                </a>
            </div>
            <div class="clearfix"></div>
        </div>
    </div>
    {% endblock %}

